#include <stdio.h>

void verificaTrian(int x, int y, int z) {
    if ((x < y + z) && (y < x + z) && (z < x + y)) {
      if (x == y && x == z) {
        printf("Equilatero");
      } else if (x == y || x == z || y == z) {
        printf("Isoceles");
      } else if (x != y != z) {
        printf("Escaleno");
      } else {
        printf("O triangulo Ã© invalido");
      }
    }
  }

  int main(void) {
    int l1, l2, l3;

    printf("Digite o primeiro valor\n");
    scanf("%d", &l1);

    printf("Digite o segundo valor\n");
    scanf("%d", &l2);

    printf("Digite o terceiro valor\n");
    scanf("%d", &l3);

    verificaTrian(l1, l2, l3);
  }
